  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '**/*.php' --dest-file 'languages/piassi.pot' --domain 'jusdocs'",
    "dev-email": "webpack --config webpack.config.emails.js --watch",
    "build-email": "webpack --config webpack.config.emails.js --mode=production"
  },
